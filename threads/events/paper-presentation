    $(document).ready(function() {
        $('#fold-clicker').click(function() {
           $('#rules').stop().slideToggle(); 
           if($('#fold-arrow img').attr('src') == 'http://10.4.3.65/threads/threads/images/down-arrow.png')
             $('#fold-arrow img').attr('src', 'http://10.4.3.65/threads/threads/images/up-arrow.png');
            else
              $('#fold-arrow img').attr('src', 'http://10.4.3.65/threads/threads/images/down-arrow.png');
           });
    });
